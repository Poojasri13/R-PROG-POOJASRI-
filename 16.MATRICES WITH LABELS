m1 <- matrix(1:20, 5, 4)
print(m1)

m2 <- matrix(1:9, 3, 3, byrow=TRUE, dimnames=list(c("R1","R2","R3"), c("C1","C2","C3")))
print(m2)

m3 <- matrix(1:4, 2, 2, byrow=FALSE, dimnames=list(c("R1","R2"), c("C1","C2")))
print(m3)

#output
m1:
     [,1] [,2] [,3] [,4]
[1,]    1    6   11   16
[2,]    2    7   12   17
[3,]    3    8   13   18
[4,]    4    9   14   19
[5,]    5   10   15   20

m2:
   C1 C2 C3
R1  1  2  3
R2  4  5  6
R3  7  8  9

m3:
   C1 C2
R1  1  3
R2  2  4
